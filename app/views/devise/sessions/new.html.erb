<div class="row">
  <div class='col s12 m8 offset-m2 l8 offset-l2'>
      <h2 class="devise-title">Log in</h2>
      
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="input-field">
          <%= f.email_field :email, autofocus: true , class: 'validate'%><br />
          <%= f.label :email, class:'active' %>
        </div>
      
        <div class="input-field">
          <%= f.password_field :password, autocomplete: "off" , class: 'validate'%><br />
          <%= f.label :password , class:'active'%>
        </div>
      
        <% if devise_mapping.rememberable? -%>
          <div class="input-field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me, class:'active' %>
          </div>
        <% end -%>
      
        <div class="actions">
          <%= f.submit "Log in", class: 'btn waves-effect waves-light' %>
          <span class= "right"><%= render "devise/shared/links" %></span>
        </div>
      <% end %>
  </div>
</div>